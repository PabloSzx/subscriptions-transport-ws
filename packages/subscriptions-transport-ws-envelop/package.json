{
  "name": "subscriptions-transport-ws-envelop",
  "version": "2.0.0",
  "description": "A websocket transport for GraphQL subscriptions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloSzx/subscriptions-transport-ws",
    "directory": "packages/subscriptions-transport-ws-envelops"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/client.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "browser-compile": "webpack --config \"./unpkg-webpack.config.js\"",
    "clean": "rimraf browser dist coverage",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover _mocha -- --exit --full-trace ./dist/test/tests.js",
    "postcoverage": "remap-istanbul --input coverage/coverage.raw.json --type lcovonly --output coverage/lcov.info",
    "lint": "tslint --format stylish --project ./tsconfig.json",
    "prepare": "bob-esbuild build",
    "test": "npm run testonly --",
    "posttest": "npm run lint",
    "testonly": "mocha --exit --reporter spec --full-trace ./dist/test/**/*.js",
    "watch": "tsc -w",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "backo2": "^1.0.2",
    "eventemitter3": "^4.0.7",
    "isomorphic-ws": "^4.0.1",
    "iterall": "^1.3.0",
    "symbol-observable": "^4.0.0"
  },
  "devDependencies": {
    "@types/backo2": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "@types/sinon": "^10.0.2",
    "@types/ws": "^7.4.7",
    "bob-esbuild": "^1.2.3",
    "bob-esbuild-cli": "^1.0.1",
    "chai": "^4.3.4",
    "graphql": "^15.5.1",
    "graphql-subscriptions": "^1.2.1",
    "istanbul": "^1.0.0-alpha.2",
    "lodash": "^4.17.21",
    "mock-socket-with-protocol": "^7.1.1",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "ws": "^8.2.0"
  },
  "peerDependencies": {
    "graphql": ">=0.10.0",
    "ws": "^7.5.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
